notification    = always
stream_output   = False
stream_error    = False
Requirements = ((Arch == "X86_64") && (regexp("CentOS7",OpSysAndVer)))
WhenToTransferOutput = ON_EXIT_OR_EVICT
OnExitRemove         = TRUE
+JobFlavour = "testmatch"
+JobType="training"
request_GPUs = 1
request_CPUs = 4
Request_memory = 8 GB
Request_disk   = 4 GB
train_eval = train

